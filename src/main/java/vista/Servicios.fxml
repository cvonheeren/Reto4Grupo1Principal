<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXCheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="main" prefHeight="767.0" prefWidth="1150.0" style="-fx-background-color: #1976d2;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplicacion.ControladorServicios">
   <children>
      <Pane fx:id="panePrincipal" prefHeight="767.0" prefWidth="1150.0" style="-fx-background-color: #fff;">
         <children>
            <Label fx:id="titulo" layoutX="75.0" layoutY="50.0" text="ELIGA LOS SERVICIOS QUE QUIERE RESERVAR:">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <ScrollPane layoutX="75.0" layoutY="100.0" prefHeight="490.0" prefWidth="615.0">
               <content>
                  <AnchorPane fx:id="paneServicios" prefHeight="485.0" prefWidth="600.0" />
               </content>
            </ScrollPane>
            <JFXCheckBox fx:id="reservarTodo" layoutX="750.0" layoutY="100.0" mnemonicParsing="false" onAction="#reservarTodo" text="Reservar todo">
               <font>
                  <Font size="18.0" />
               </font>
            </JFXCheckBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
